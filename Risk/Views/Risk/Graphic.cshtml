@{
    ViewBag.Title = "Graphic";
}
<h2>Graphic</h2>
<div id="botonera" class="pull-right">
    <input type="button" class="btn btn-primary" name="BtnPrint" value="PRINT" id="BtnPrint" />
    @*<a href="@Url.Action("GeneraReport", "Reports")" class="btn btn-primary">HAZ PDF</a>*@
</div>
<div id="grafico" style="height: 250px;"></div>

<script>
    $(document).ready(function () {
        $.getJSON('/Risk/pintaGrafico',
           function (data) {
               Morris.Line({
                   element: 'grafico',
                   data: data,
                   xkey: 'year',
                   ykeys: ['value'],
                   labels: ['value']
               });
           }
        );

    });
</script>

<script type="text/javascript" src="~/Content/js/botoneraPartials.js"></script>
<script type="text/javascript" src="~/Content/js/scripts2.js"></script>